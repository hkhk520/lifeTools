/* pages/tips/tips.wxss */
#editor{
  width: 100%;
  height: 100%;
  padding: 20rpx;
  background-color: white;
  border-top: 1rpx solid #ddd;
}

.btns{
  width: 100%;
  height: 80rpx;
  /* background-color: lightcoral; */
  position: fixed;
  left: 0;
  bottom: 22rpx;

  margin-top: 80rpx;

  display: flex;
  justify-content: space-around;
  align-items: center;
}
.btns>view{
  width: 50rpx;
  font-size: 43rpx;
  text-align: center;
  font-weight: bold;
}
.btns>image{
  width: 40rpx;
  height: 40rpx;
}
.btns>image:nth-child(2){
  width: 42rpx;
  height: 42rpx;
}
.btns>image:nth-child(4){
  width: 38rpx;
  height: 38rpx;
}
.btns>image:nth-child(5){
  width: 38rpx;
  height: 38rpx;
}

button{
  position: fixed;
  left: 50%;
  top: 50%;
}
